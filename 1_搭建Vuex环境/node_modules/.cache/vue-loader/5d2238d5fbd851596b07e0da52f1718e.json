: {\n        addNow () {\n            this.curCount += this.addCount\n        },\n        addOdd () {\n            if (this.curCount % 2) {\n                this.curCount += this.addCount\n            }\n        },\n        addWait () {\n            setTimeout(() => {\n                this.curCount += this.addCount\n            }, 1000)\n        }\n    }\n}\n</script>\n\n<style>\nbutton {\n    margin: 0 5px;\n}\n</style>"]}]}